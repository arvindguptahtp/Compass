    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "chalk": "^2.0.1",
    "jasmine-core": "^2.6.4",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.4",
    "nightwatch": "^0.9.16",
    "phantomjs-prebuilt": "^2.1.7",
    "selenium-download": "^2.0.9"
  }
}
