{
  "name": "VEFA-Django",
  "description": "VEFA front-end webpack builder for static prototyping and Django integration",
  "author": "Wellfire Interactive",
  "license": "private",
  "private": true,
  "repository": {},
  "scripts": {
    "dev": "webpack --progress --colors --watch --env.init=vefa.yaml",
    "build": "webpack -p --progress --colors --env.production --env.init=vefa.yaml",
    "assets": "webpack -p --progress --colors --env.django --env.production --env.init=vefa.django.yaml --env.tasks=assets",
    "vue-dev": "webpack --progress --colors --watch --env.django --env.init=vefa.django.yaml --env.tasks=vue",
    "vue-build": "webpack -p --progress --colors --env.django --env.production --env.init=vefa.django.yaml --env.tasks=vue",
    "template-dev": "webpack --progress --colors --watch --env.django --env.init=vefa.django.yaml --env.tasks=html",
    "template-build": "webpack -p --progress --colors --env.django --env.production --env.init=vefa.django.yaml --env.tasks=html",
    "django-dev": "webpack --progress --colors --watch --env.django --env.init=vefa.django.yaml --env.tasks=dev",
    "django-build": "webpack -p --progress --colors --env.django --env.production --env.init=vefa.django.yaml",
    "postbuild": "node vefa/razor.js --config=vefa.yaml",
    "postdjango-build": "node vefa/razor.js --config=vefa.django.yaml",
    "test": "karma start karma.config.js --progress --colors -p --init vefa.yaml"
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.22.0",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "css-loader": "^0.28.1",
    "css-mqpacker": "^6.0.0",
    "css-razor": "^2.4.0",
    "cssnano": "^3.10.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "image-webpack-loader": "^3.3.0",
    "imagemin-pngquant": "^5.0.1",
    "js-yaml": "^3.9.0",
    "loader-utils": "^1.1.0",
    "markdown-it": "^8.3.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^4.0.0",
    "markdown-it-attrs": "^0.9.0",
    "markdown-it-block-image": "0.0.3",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-footnote": "^3.0.1",
    "null-loader": "^0.1.1",
    "postcss-brand-colors": "^1.0.0",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "postcss-pxtorem": "^4.0.1",
    "pug": "^2.0.0-rc.2",
    "rucksack-css": "^0.9.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "typogr": "^0.6.7",
    "vefa-asset-loader": "vefari/vefa-asset-loader",
    "vefa-export-loader": "vefari/vefa-export-loader",
    "vefa-html-loader": "vefari/vefa-html-loader",
    "vue": "^2.3.4",
    "vue-loader": "^13.0.1",
    "vue-template-compiler": "^2.3.4",
    "webp-loader": "^0.2.0",
    "webpack": "^2.5.0",
