extends /site/detail

block props
    -   
        profile = new Vefa()
            .data(programs.fixtures[2])
        
        profile = profile.$freeze()
        
        page.data({
            title: profile.data.name,
            slug: routes.programs.slug,
            icon: "programs"
        })


prepend preview
    -
        poster = new Vefa()
            .mix({
                element: {
                    class: "co--programs",
                }
            })
            .data({
                contact: {
                    url: "http://communitiesinschools.org",
                }
            })


